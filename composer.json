{
    "name": "marceli-to/statamic-sync",
    "description": "Sync Statamic content and assets from remote environments to local.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "marceli-to",
            "email": "m@marceli.to"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MarceliTo\\StatamicSync\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarceliTo\\StatamicSync\\StatamicSyncServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
